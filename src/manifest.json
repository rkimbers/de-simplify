{
    "manifest_version": 3,
    "name": "de-simpify",
    "version": "1.0",
    "description": "Strips Simplify.jobs query parameters from URLs before navigation.",
    "permissions": [
      "webRequest",
      "webRequestBlocking"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }